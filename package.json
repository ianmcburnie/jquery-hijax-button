{
  "name": "jquery-hijax-button",
  "description": "jQuery collection plugin that changes anchor tag semantics and behaviour to button semantics and behaviour",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run lasso && parallelshell 'npm run server' 'npm run watch'",
    "tdd": "karma start --autoWatch true --singleRun false",
    "test": "karma start --autoWatch false --singleRun true",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "build": "npm run jsdoc && npm run minify && npm run lasso",
    "clean": "rimraf jsdoc test_reports .cache build docs/browser.js.* lint.txt",
    "fix": "eslint jquery.hijaxbutton.js --fix",
    "jsdoc": "jsdoc jquery.hijaxbutton.js -d jsdoc",
    "lasso": "lasso browser.json --output-dir docs --minify",
    "lint": "eslint jquery.hijaxbutton.js > lint.txt",
    "minify": "jsmin -o jquery.hijaxbutton.min.js jquery.hijaxbutton.js",
    "server": "browser-sync start -s --ss docs --index docs/index.html --files docs ",
    "watch": "onchange jquery.hijaxbutton.js -- npm run lasso"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-hijax-button",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-hijax-button/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebayui",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-hijax-button.git"
  },
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2",
    "coveralls": "^2",
    "eslint": "^3",
    "eslint-config-google": "~0.6",
    "jasmine-core": "^2",
    "jquery": ">=1.11",
    "jquery-migrate": "^3",
    "jsdoc": "^3",
    "jsmin": "^1",
    "karma": "^1",
    "karma-coverage": "^1",
    "karma-html-reporter": "~0.2",
    "karma-jasmine": "^1",
    "karma-junit-reporter": "^1",
    "karma-phantomjs-launcher": "^1",
    "lasso-cli": "^2",
    "onchange": "^3",
    "parallelshell": "^2",
    "phantomjs-prebuilt": "^2",
    "pre-commit": "^1",
    "rimraf": "^2"
  },
  "peerDependencies": {
    "jquery": ">=1.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
